<div class="row">
  <div class="container">
    <h1>Create A New Competition</h1>
    <%= form_for :competition, url: competitions_path do |f| %>
  <p>
    <%= f.label :start_date %><br>
    <%= f.text_field :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br>
    <%= f.text_field :end_date %>
  </p>
  <p>
    <%= f.label :location %><br>
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.label :team_names %><br>
    <%= f.text_field :team_names %>
  </p>
  <p>
    <%= f.label :about %><br>
    <%= f.text_area :about %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
  </div>
</div>
